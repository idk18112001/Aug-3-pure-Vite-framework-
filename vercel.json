{
  "functions": {
    "api/auth/google.ts": {
      "runtime": "nodejs18.x"
    },
    "api/auth/callback/google.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/google",
      "destination": "/api/auth/google"
    },
    {
      "source": "/api/auth/callback/google",
      "destination": "/api/auth/callback/google"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}